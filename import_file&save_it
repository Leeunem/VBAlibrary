'#### Import file ###
Dim WBo As Workbook
Dim WBpath As String
Dim WBn As Workbook

Set WBo = ActiveWorkbook

On Error GoTo ExitTheSub
WBpath = Application.GetOpenFilename()
Workbooks.Open (WBpath)
Set WBn = ActiveWorkbook
Application.ScreenUpdating = False
Application.DisplayAlerts = False
On Error GoTo 0

ChDir _
        "Z:\0 GCM CLIENT SERVICES\ACTIVE CREDIT LINE SHEET (retain +11 Years as of 2016)\Data"
    ActiveWorkbook.SaveAs Filename:= _
        "Z:\0 GCM CLIENT SERVICES\ACTIVE CREDIT LINE SHEET (retain +11 Years as of 2016)\Data\DealsT.xls" _
        , FileFormat:=xlExcel8, Password:="", WriteResPassword:="", _
        ReadOnlyRecommended:=False, CreateBackup:=False
        
WBn.Close
Application.DisplayAlerts = True
Application.ScreenUpdating = True

ExitTheSub:
'End Sub

'###Options
'Kill WBpath 'Delete the sfile on the hardrive
'ActiveSheet.Name = "SHEET_NAME"
